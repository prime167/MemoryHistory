<Window x:Class="MemoryHistoryWpf.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MemoryHistoryWpf"
        mc:Ignorable="d"
        Title="MemHis" Height="600" Width="1200" Loaded="MainWindow_OnLoaded">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <WpfPlot Name="WpfPlot1" Grid.Row="0" Grid.Column="0"></WpfPlot>
        <WpfPlot Name="WpfPlot2" Grid.Row="0" Grid.Column="1"></WpfPlot>
        <WpfPlot Name="WpfPlot3" Grid.Row="1" Grid.Column="0"></WpfPlot>
        <WpfPlot Name="WpfPlot4" Grid.Row="1" Grid.Column="1"></WpfPlot>
        <WpfPlot Name="WpfPlot5" Grid.Row="2" Grid.Column="0"></WpfPlot>
        <WpfPlot Name="WpfPlot6" Grid.Row="2" Grid.Column="1"></WpfPlot>
    </Grid>
</Window>
